public class MaxNumber {
	public static void main(String[] args) {
		int i,j,k,n;
		System.out.println("parcissus numbers are ");
		for(n = 100;n < 1000;n++){
			i = n/100;
			j = n/10 - i * 10;
			k = n%10;
			if(n == i*i*i + j*j*j + k*k*k) 
				System.out.print(n + " ");
		}
		System.out.println();
	}
}

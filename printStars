import java.util.Scanner;
public class MaxNumber {
	public static void main(String[] args) {
		int i,j,k;
		for(i = 0;i <= 3;i++){
			for(j = 0;j <= 2 - i;j++)
				System.out.print(" ");
			for(k = 0;k <= 2*i;k++)
				System.out.print("*");
			System.out.println();
		}
		for(i = 0;i <= 2;i++){
			for(j = 0;j <= i;j++)
				System.out.print(" ");
			for(k = 0;k <= 4 - 2*i;k++)
				System.out.print("*");
			System.out.println();
		}
	}
}

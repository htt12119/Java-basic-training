/**
 *求一个3*3的整形矩阵对角线元素之和
 */
import java.util.Scanner;
public class Enjoyment {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int a[][] = new int[3][3];
		int sum = 0,i,j;
		System.out.println("Enter data:");
		for(i = 0;i < 3;i++)
			for(j = 0;j < 3;j++)
				a[i][j] = input.nextInt();
		for(i = 0;i < 3;i++)
			sum = sum + a[i][i];
		System.out.printf("sum = %6d\n",sum);
	}
}
